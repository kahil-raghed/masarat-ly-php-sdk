{
    "name": "kahil-raghed/masarat-ly-php-sdk",
    "description": "Package to process payments throught https://masarat.ly payment gateway",
    "homepage": "https://github.com/kahil-raghed/masarat-ly-php-sdk",
    "type": "library",
    "keywords": ["payment", "masarat", "sdk", "php"],
    "require": {
        "php": "^7.4||^8.0",
        "guzzlehttp/guzzle": "^6.5||^7.0"
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KahilRaghed\\MasaratLy\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Raghed Kahil",
            "email": "kahilraghed@gmail.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^4.3"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
